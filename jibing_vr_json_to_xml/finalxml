<item>
	<key><![CDATA[1094]]></key>
	<display>
		<disease><![CDATA[气胸]]></disease>
	  	<other_name><![CDATA[]]></other_name>
	  	<videoUrl><![CDATA[http://m.baikemy.com/video/31161149455617]]></videoUrl>
