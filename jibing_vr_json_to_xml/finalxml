<item>
	<key><![CDATA[1088]]></key>
	<display>
		<disease><![CDATA[上呼吸道感染]]></disease>
	  	<other_name><![CDATA[ð]]></other_name>
	  	<videoUrl><![CDATA[http://m.baikemy.com/video/31202865869825]]></videoUrl>
